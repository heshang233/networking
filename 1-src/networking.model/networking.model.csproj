    <AssemblyName>Networking.Model</AssemblyName>
    <RootNamespace>Networking.Model</RootNamespace>
  </PropertyGroup>

  <PropertyGroup>
    <PackageId>Networking.Model</PackageId>
    <Version>$(Networking_Model_Version)</Version>
